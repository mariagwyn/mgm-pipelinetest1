{
   "name": "cantemus",
   "version": "0.1.0",
   "description": "",
   "author": "Greg Johnston <greg.johnston@gmail.com>",
   "license": "MIT",
   "engines": {
      "node": "13.2.0"
   },
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "tsc -p tsconfig.build.json",
      "prestart:prod": "rimraf dist && npm run build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/main.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/src/main.js",
      "lint": "tslint -p tsconfig.json -c tslint.json",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "postinstall": "npm run prestart:prod"
   },
   "dependencies": {
      "@azure/functions": "^1.0.3",
      "@nestjs/azure-func-http": "^0.3.2",
      "@nestjs/cli": "^6.13.3",
      "@nestjs/common": "^6.10.14",
      "@nestjs/config": "^0.1.0",
      "@nestjs/core": "^6.10.14",
      "@nestjs/platform-express": "^6.10.14",
      "@nestjs/typeorm": "^6.2.0",
      "@nestjsx/crud": "^4.4.1",
      "@nestjsx/crud-request": "^4.4.1",
      "@nestjsx/crud-typeorm": "^4.4.1",
      "@okta/jwt-verifier": "^1.0.0",
      "@okta/okta-auth-js": "^2.11.2",
      "@okta/okta-sdk-nodejs": "^3.1.0",
      "bcryptjs": "^2.4.3",
      "bible-passage-reference-parser": "^2.0.1",
      "class-transformer": "^0.3.1",
      "class-validator": "^0.11.0",
      "docx": "^5.0.0-rc7",
      "mysql": "^2.14.1",
      "node-html-parser": "^1.1.18",
      "pg": "^7.18.1",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.0",
      "rxjs": "^6.5.4",
      "sqlite3": "^4.1.1",
      "ts-node": "^8.6.1",
      "typeorm": "^0.2.21",
      "typescript": "^3.7.4"
   },
   "devDependencies": {
      "@nestjs/schematics": "^6.8.2",
      "@nestjs/testing": "^6.10.14",
      "@types/express": "^4.17.1",
      "@types/jest": "^24.0.25",
      "@types/node": "^8.0.29",
      "@types/supertest": "^2.0.8",
      "jest": "^24.9.0",
      "prettier": "^1.18.2",
      "supertest": "^4.0.2",
      "ts-jest": "^24.3.0",
      "ts-loader": "^6.1.1",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^5.20.0",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
